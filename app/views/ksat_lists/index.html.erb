<h1>Listing ksat_lists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @ksat_lists.each do |ksat_list| %>
      <tr id="<%= dom_id(ksat_list) %>">
        <td><%= ksat_list.title %></td>
        <td><%= ksat_list.description %></td>
        <td>
          <%= link_to "List Items", ksat_list_task_items_path(ksat_list) %>
          <%= link_to 'Show', ksat_list %>
          <%= link_to 'Edit', edit_ksat_list_path(ksat_list) %>
          <%= link_to 'Destroy', ksat_list, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ksat list', new_ksat_list_path %>
